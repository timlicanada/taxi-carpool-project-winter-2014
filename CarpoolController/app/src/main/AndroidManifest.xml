<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.jeremy.carpoolcontroller" >

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="com.google.android.providers.gsf.permission.READ_GSERVICES" />
    <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION" />
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />
    <uses-permission android:name="myapp.permission.MAPS_RECEIVE" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <meta-data
            android:name="com.google.android.gms.version"
            android:value="@integer/google_play_services_version" />
        <meta-data
            android:name="com.google.android.maps.v2.API_KEY"
            android:value="@string/google_maps_key" />
        <meta-data
            android:name="com.google.android.gms.version"
            android:value="@integer/google_play_services_version" />

        <activity
            android:name="com.example.jeremy.carpoolcontroller.CarpoolInfo"
            android:label="@string/title_activity_carpool_info" >
        </activity>
        <activity
            android:name="com.example.jeremy.carpoolcontroller.CarMenu2"
            android:label="@string/title_activity_car_menu2" >
        </activity>
        <activity
            android:name="com.example.jeremy.carpoolcontroller.LoginError"
            android:label="@string/title_activity_login_error" >
        </activity>
        <activity
            android:name="com.example.jeremy.carpoolcontroller.LogoutActivity"
            android:label="@string/title_activity_logout" >
        </activity>
        <activity
            android:name="com.example.jeremy.carpoolcontroller.MainActivity"
            android:label="@string/title_activity_main" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.example.jeremy.carpoolcontroller.StartMenu"
            android:label="@string/title_activity_start_menu" >
        </activity>
        <activity
            android:name="com.example.jeremy.carpoolcontroller.LoginActivity"
            android:label="@string/title_activity_login" >
        </activity>
        <activity
            android:name="com.example.jeremy.carpoolcontroller.RegistrationPage"
            android:label="@string/title_activity_registration_page" >
        </activity>
        <activity
            android:name="com.example.jeremy.carpoolcontroller.LocalProfilePage"
            android:label="@string/title_activity_local_profile_page" >
        </activity>
        <activity
            android:name="com.example.jeremy.carpoolcontroller.UserSearchPage"
            android:label="@string/title_activity_user_search_page" >
        </activity>
        <activity
            android:name="com.example.jeremy.carpoolcontroller.PublicProfilePage"
            android:label="@string/title_activity_public_profile_page" >
        </activity>
    </application>

</manifest>
